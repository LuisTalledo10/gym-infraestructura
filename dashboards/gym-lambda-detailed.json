      },
      {
        "id": 2,
        "title": "User Management - Invocations",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 1},
        "targets": [
          {
            "refId": "A",
            "datasource": {"type": "cloudwatch", "uid": "${DS_CLOUDWATCH}"},
            "region": "us-east-1",
            "namespace": "AWS/Lambda",
            "metricName": "Invocations",
            "dimensions": {"FunctionName": "gym-user-management"},
            "statistics": ["Sum"],
            "period": "300"
          }
        ]
